{
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"rules": {
		"react/prop-types": "off"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"env": {
    "browser": true,
    "node": true,
    "jasmine": true
  }
}
